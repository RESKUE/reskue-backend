# content-database
CONTENT_DB=content_db
CONTENT_DB_PORT=5433
CONTENT_DB_USER=reskue
CONTENT_DB_PASSWORD=1234

# auth-database
AUTH_DB=auth_db
AUTH_DB_PORT=5434
AUTH_DB_USER=reskue
AUTH_DB_PASSWORD=1234

# location-database
LOCATION_DB=location_db
LOCATION_DB_PORT=5435
LOCATION_DB_USER=reskue
LOCATION_DB_PASSWORD=1234

# Postgres
POSTGRES_INITDB_ARGS=--auth-local=password
POSTGRES_HOST_AUTH_METHOD=password
DB_VENDOR=POSTGRES
