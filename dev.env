# Spring
SPRING_PORT=8080
ARTIFACT_ID=backend
VERSION=0.0.1-SNAPSHOT
LOGGING_LEVEL_ORG_SPRINGFRAMEWORK=ERROR
SPRING_JPA_HIBERNATE_DDL-AUTO=create-drop
SPRING_DATASOURCE_INITIALIZATION-MODE=always
SPRING_DATASOURCE_PLATFORM=postgres
SPRING_DATASOURCE_PLATFORM_JDBC=postgresql
KUERES_MEDIA_DIR=/media-dir
KUERES_TOPIC-EXCHANGE=reskue-events
KUERES_DEFAULT-QUEUE=event-consumer